FoamFile
{
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{

 frontHeated
    {
    	type zeroGradient;
    
    }
    
    backOutlet
    {
    	type 		inletOutlet;
    	inletValue	uniform 0;
    	value		uniform 0;
    }

 defaultFaces
    {
    type            empty;
    }

}

