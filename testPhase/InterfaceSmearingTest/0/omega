FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    frontHeated
    {
    	type            omegaWallFunction;
        value           uniform 0.1;
    
    }
        
    defaultFaces
    {
        type            empty;
    }

}


