FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 1
);

regions
(
    boxToCell
    {
        box (0 0 0) (.24 1 1);
        fieldValues
        (
            volScalarFieldValue alpha.water 0
        );
    }
    boxToCell
    {
        box (.24 0 0) (.26 1 1);
        fieldValues
        (
            volScalarFieldValue alpha.water .5
        );
    }
);


